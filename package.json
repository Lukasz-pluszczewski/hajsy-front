  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "node tools/distServer.js",
    "predev": "npm run start-message & npm run clean-dist",
    "dev": "node tools/srcServer.js",
    "lint": "esw webpack.config.* src tools test features --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rm -rf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "node tools/build.js",
    "test": "NODE_PATH=src mocha tools/testSetup.js \"./test/**/*.js\"",
    "test:cucumber": "rm -rf ./screenshots && NODE_PATH=src wdio wdio.conf.js",
    "test:coverage": "NODE_PATH=src babel-node ./node_modules/.bin/isparta cover _mocha -- --require ./tools/testSetup.js \"./test/**/*.js\" && open coverage/lcov-report/index.html",
    "test:watch": "npm run test -- --watch",
    "selenium:install": "selenium-standalone install",
    "selenium": "selenium-standalone start"
  },
